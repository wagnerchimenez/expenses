{
    "name": "Docker Expenses App",
    "dockerComposeFile": [
        "../docker-compose.yml"
    ],
    "service": "expenses",
    "extensions": [
        "Dart-Code.dart-code",
        "Dart-Code.flutter"
    ],
    "settings": {
        "dart.flutterSdkPath": "/usr/local/flutter",
        "dart.sdkPath": "/usr/local/flutter/bin/cache/dart-sdk"
    },
    "workspaceFolder": "/expenses",
    "forwardPorts": [
        80
    ]
}